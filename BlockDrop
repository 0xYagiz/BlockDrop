local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, IntroEnabled = false})

OrionLib:MakeNotification({
	Name = "Loged In",
	Content = "You Are Loged In Sucsesfully as "..Player.Name.." ",
	Image = "rbxassetid://4483345998",
	Time = 5
})

--Values
_G.Key = "ygz505"
_G.KeyInput = "string"

_G.autoDrop = true

_G.autoSpin = true

_G.autoChat = true

_G.autoRoom = true

--Functions

function autoRoom()
	while _G.autoRoom == true do
	-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "BlockDrop",
    [2] = 10,
    [3] = {
        ["assetType"] = "GamePass",
        ["assetId"] = "30045417"
    }
}

game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.Tickets.CreateRoom:InvokeServer(unpack(args))
	wait(5)
end
end

function autoDestroyRoom()
	-- Script generated by SimpleSpy - credits to exx#9394

game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.Tickets.DestroyRoom:InvokeServer()
end


function autoChat()
	while _G.autoChat == true do
		local args = {
    [1] = "block drop 10 rbx",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

		wait(40)
	end
end

function autoSpin()
	while _G.autoSpin == true do
		-- Script generated by SimpleSpy - credits to exx#9394

game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.Tickets.DestroyRoom:InvokeServer() 
		-- Script generated by SimpleSpy - credits to exx#9394
		

game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.DailySpinner.ClaimDailySpinner:InvokeServer()

		wait(50)
	end
end

function autoDrop()
	while _G.autoDrop == true do
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Sw1ndlerScripts/RobloxScripts/main/Misc%20Scripts/DoubleDown.lua?raw=true"))()
		wait(3)
	end
end

function CorrectKeyNotification()
	OrionLib:MakeNotification({
	Name = "Correct Key!",
	Content = "You have entered the correct key!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end

function IncorrectKeyNotification()
	OrionLib:MakeNotification({
	Name = "incorrect Key!",
	Content = "You have entered the Incorrect key!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end

--FarmTabs

local FarmTab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FarmTab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

FarmTab:AddButton({
	Name = "Check Key",
	Callback = function()
      	if _G.KeyInput == _G.Key then
  		MakeScriptHub()
		CorrectKeyNotification()
		else
			IncorrectKeyNotification()
		end
	end    
})


