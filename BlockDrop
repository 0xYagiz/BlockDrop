function  MakeScriptHub()
	local Window = OrionLib:MakeWindow({Name = "YznHub| [BACK] Double Down ðŸŽ²", HidePremium = false, SaveConfig = true, IntroEnabled = true, IntroText = "YznHub"})
	local FarmTab = Window:MakeTab({
	Name = "Block Drop",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FarmTab:AddToggle({
	Name = "Auto Drop",
	Default = false,
	Callback = function(Value)
		_G.autoDrop = Value
		autoDrop()
	end    
})
	
		local FarmTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FarmTab:AddToggle({
	Name = "UÃ‡MAK FELAN",
	Default = false,
	Callback = function(Value)
		_G.autoDrop = Value
		autoDrop()
	end    
})

end
